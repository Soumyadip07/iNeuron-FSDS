   ],
   "source": [
    "f=0\n",
    "for i in range(2,n-1):\n",
    "    if(n%i==0):\n",
    "        f=f+1\n",
    "        break\n",
    "if(f>0):\n",
    "    print(n,\"is not a prime number\")\n",
    "else:\n",
    "    print(n,\"is a prime number\")"
