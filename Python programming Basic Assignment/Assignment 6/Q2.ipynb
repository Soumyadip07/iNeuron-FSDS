   "source": [
    "def fact(a):\n",
    "    if(a==1):\n",
    "        return(a)\n",
    "    else:\n",
    "        return a*fact(a-1)\n",
    "\n",
    "n=int(input('Enter a number:- '))\n",
    "if(n<0):\n",
    "    print('Please enter a positive number!')\n",
    "elif(n==0):\n",
    "    print('The factorial of 0 is 1')\n",
    "else:\n",
    "    print('The factorial of',n,'is:',fact(n))"
   ]
  }
